  int r = 255;
  int g = 255;
  int b = 255;
  for (int i = 0; i < nbLeds; i++) {
    strip.setPixelColor(i, r, g, b);
  }
  strip.show();
}

void turn(RGB rgb,int i) {
    strip.setPixelColor(i, rgb.r, rgb.g, rgb.b);
}

void turnOff() {
  for (int i = 0; i < nbLeds; i++) {
    strip.setPixelColor(i, 0);
  }
  strip.show();
}



